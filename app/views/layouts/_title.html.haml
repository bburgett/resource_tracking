%title
  - title = yield(:title) || " : Resource Tracking Database "
  = "Resource Tracking Database" + " : " + title
= javascript_include_tag :defaults, 'jquery-1.4.2.min', 'jquery-ui-1.8.4.custom.min', 'jquery.validateClassificationTree', 'jquery.collapsibleCheckboxTree', 'jquery.tipsy', 's3Slider', "jquery.effects.core", "jquery.effects.highlight.js", :cache => true
= record_select_includes
= active_scaffold_includes

= stylesheet_link_tag 'compiled/screen.css', :media => 'screen, projection'
= stylesheet_link_tag 'compiled/print.css', :media => 'print'
= stylesheet_link_tag 'custom', 'tipsy', 'ui-lightness/jquery-ui-1.8.4.custom'

/[if lt IE 8]
  = stylesheet_link_tag 'compiled/ie.css', :media => 'screen, projection'


